<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.zone.user.entity.Admin" table="admin">
        <id name="adminid" column="adminid">
            <generator class="native"></generator>
            
        </id>
        <property name="adminname" column="adminname" length="50" not-null="true"/>
        <property name="password" column="userpass" length="50" not-null="true"/>
        <property name="state" column="state" length="2" not-null="true"/>
    </class>
</hibernate-mapping>